{
    "loginRequired": true,
    "password": "hello",
    "host": "openshift",
    "database": "MongoDB",
    "saveLimit": 100000,
    "imgsPath": "server/resources/public_imgs",
    "id": 1,
    "nodes": [],
    "imageSaver":{
        "enabled": true,
        "sleepTime": 3600000,
        "timeGap": 1000,
        "sources": [
            {
                "name": "local",
                "enabled": false,
                "step": 20,
                "path": "server/resources/imgs/",
                "publicPath": "server/resources/public_imgs"
            },
            {
                "name": "flickr",
                "enabled": true,
                "step" : 30,
                "wordLists": ["en-US.txt", "de-DE.txt"],
                "randomization": 0.5
            }
        ]
    },
    "searchRequest":{
        "similarityThreshold": 85,
        "defaultDistance": "chisquare",
        "maxResult": 5000
    },
    "index":{
        "lshEnabled": true,
        "max": 100000,
        "dimensions": 199,
        "delta": 0.1,
        "k": 5,
        "w": 0.4,
        "L": 60,
        "family": "chi2"
    }
}